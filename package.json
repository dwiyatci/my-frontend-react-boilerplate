{
  "name"           : "my-frontend-react-boilerplate",
  "version"        : "0.0.1",
  "description"    : "My ReactJS boilerplate for developing frontend UI application",
  "license"        : "SEE LICENSE IN LICENSE.txt",
  "author"         : "Glenn Dwiyatcita (https://tiny.cc/dwiyatci)",
  "dependencies"   : {
    "bootstrap"    : "4.0.0-alpha.2",
    "font-awesome" : "4.5.0",
    "jquery"       : "2.2.2",
    "kendo-ui-core": "1.0.2",
    "lodash"       : "4.6.1",
    "react"        : "0.14.7",
    "react-dom"    : "0.14.7",
    "react-router" : "2.0.1",
    "tether"       : "1.2.0"
  },
  "devDependencies": {
    "babel-core"                    : "6.7.2",
    "babel-loader"                  : "6.2.4",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-polyfill"                : "6.7.2",
    "babel-preset-es2015"           : "6.6.0",
    "babel-preset-react"            : "6.5.0",
    "babel-preset-stage-2"          : "6.5.0",
    "bootstrap-loader"              : "1.0.10",
    "css-loader"                    : "0.23.1",
    "express"                       : "4.13.4",
    "extract-text-webpack-plugin"   : "1.0.1",
    "file-loader"                   : "0.8.5",
    "font-awesome-loader"           : "0.0.1",
    "html-loader"                   : "0.4.3",
    "html-webpack-plugin"           : "2.10.0",
    "imports-loader"                : "0.6.5",
    "jscs"                          : "2.11.0",
    "json-loader"                   : "0.5.4",
    "less"                          : "2.6.1",
    "node-sass"                     : "3.4.2",
    "postcss"                       : "5.0.19",
    "postcss-loader"                : "0.8.2",
    "resolve-url-loader"            : "1.4.3",
    "rimraf"                        : "2.5.2",
    "sass-loader"                   : "3.2.0",
    "style-loader"                  : "0.13.0",
    "updtr"                         : "0.1.7",
    "url-loader"                    : "0.5.7",
    "webpack"                       : "1.12.14",
    "webpack-dev-server"            : "1.14.1"
  },
  "scripts"        : {
    "setup"        : "npm install rimraf updtr webpack webpack-dev-server jscs -g",
    "install:clean": "rimraf node_modules && npm install",
    "clean"        : "rimraf dist",
    "webpack"      : "webpack",
    "watch"        : "webpack -w",
    "start"        : "webpack-dev-server --inline --hot",
    "jscs"         : "jscs src",
    "build"        : "npm run jscs && npm run clean && webpack --config webpack.prod.config.js",
    "update"       : "updtr",
    "update:global": "npm run setup"
  },
  "repository"     : {
    "type": "git",
    "url" : "https://github.com/dwiyatci/my-frontend-react-boilerplate"
  }
}
